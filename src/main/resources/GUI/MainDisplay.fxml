<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<BorderPane fx:id="basePane" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="untimelyRock.gui.MainDisplayController">
   <top>
      <AnchorPane maxHeight="-Infinity" prefHeight="15.0" prefWidth="600.0" style="-fx-background-color: grey;" BorderPane.alignment="TOP_LEFT">
         <children>
            <MenuBar prefWidth="132.0" style="-fx-background-color: Dark Grey;" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0" AnchorPane.topAnchor="0.0">
              <menus>
                <Menu mnemonicParsing="false" text="File">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Close" />
                    <MenuItem mnemonicParsing="false" onAction="#openPackToEdit" text="Open" />
                    <MenuItem mnemonicParsing="false" text="New" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Edit">
                  <items>
                    <MenuItem mnemonicParsing="false" text="Delete" />
                  </items>
                </Menu>
                <Menu mnemonicParsing="false" text="Help">
                  <items>
                    <MenuItem mnemonicParsing="false" text="OpenTestPack(Not For Production)" />
                  </items>
                </Menu>
                  <Menu mnemonicParsing="false" text="Tools">
                    <items>
                      <MenuItem mnemonicParsing="false" text="Texture Editor" />
                        <MenuItem mnemonicParsing="false" text="Audio Editor" />
                    </items>
                  </Menu>
              </menus>
            </MenuBar>
         </children></AnchorPane>
   </top>
   <center>
      <AnchorPane fx:id="editorAnchor" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
</BorderPane>
